{
  "name": "Jira Toggl MD",
  "description": "A web extension for syncing Toggl timelog entries to your Jira Software worklog",
  "version": null,
  "permissions": [
    "https://*/",
    "http://*/",
    "storage"
  ],
  "applications": {
    "gecko": {
      "id": "{5c665328-f549-4cca-9f6b-cc149720aea6}"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "icons": {
    "16": "icons/jira-toggl_16.png",
    "48": "icons/jira-toggl_48.png",
    "128": "icons/jira-toggl_128.png"
  },
  "browser_action": {
    "default_title": "jira-toggl",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
